# NPM Configuration for Fixzit Project

# Suppress warning noise (Turbopack/Webpack warning is expected - see TURBOPACK_WARNING_EXPLAINED.md)
# Note: This doesn't actually suppress the Next.js warning, but documents our awareness of it

# Use legacy peer deps to avoid conflicts
legacy-peer-deps=false

# Strict engine check
engine-strict=false

# Package lock
package-lock=true

# Save exact versions
save-exact=false
