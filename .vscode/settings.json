{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.validate.enable": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "typescript.tsserver.projectConfigPaths": [
    "tsconfig.json",
    "tsconfig.vitest.json"
  ],
  "javascript.validate.enable": true,
  "eslint.enable": true,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "problems.decorations.enabled": true,
  "problems.showCurrentInStatus": true,
  "problems.autoReveal": true,
  "github-actions.workflows.pinned.workflows": [],
  "github-actions.workflows.pinned.refresh.enabled": false,
  "github-actions.org-features": true,
  "github-actions.remote-name": "origin",
  "github-actions.workflows.validation.enabled": true,
  "github-actions.workflows.secrets": [
    "AUTH_SECRET",
    "BULLMQ_REDIS_URL",
    "DEFAULT_ORG_ID",
    "GITHUB_TOKEN",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "HEALTH_CHECK_TOKEN",
    "JWT_SECRET",
    "JWT_SECRET_TEST",
    "MONGODB_URI",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_SECRET_TEST",
    "NEXTAUTH_URL",
    "OPENAI_KEY",
    "PAYTABS_PROFILE_ID",
    "PAYTABS_SERVER_KEY",
    "PUBLIC_ORG_ID",
    "REDIS_URL",
    "RENOVATE_TOKEN",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "STORE_PATH",
    "TEST_ADMIN_EMAIL",
    "TEST_ADMIN_EMPLOYEE",
    "TEST_ADMIN_PASSWORD",
    "TEST_FINANCE_OFFICER_EMAIL",
    "TEST_FINANCE_OFFICER_EMPLOYEE",
    "TEST_FINANCE_OFFICER_PASSWORD",
    "TEST_HR_OFFICER_EMAIL",
    "TEST_HR_OFFICER_EMPLOYEE",
    "TEST_HR_OFFICER_PASSWORD",
    "TEST_OPERATIONS_MANAGER_EMAIL",
    "TEST_OPERATIONS_MANAGER_EMPLOYEE",
    "TEST_OPERATIONS_MANAGER_PASSWORD",
    "TEST_ORG_ID",
    "TEST_SUPPORT_AGENT_EMAIL",
    "TEST_SUPPORT_AGENT_EMPLOYEE",
    "TEST_SUPPORT_AGENT_PASSWORD",
    "TEST_SUPPORT_AGENT_USER_ID",
    "TEST_TEAM_MEMBER_EMAIL",
    "TEST_TEAM_MEMBER_EMPLOYEE",
    "TEST_TEAM_MEMBER_PASSWORD",
    "TEST_TEAM_MEMBER_USER_ID",
    "ZATCA_API_KEY",
    "ZATCA_SELLER_ADDRESS",
    "ZATCA_SELLER_NAME",
    "ZATCA_VAT_NUMBER"
  ],
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/*.yml"
  },
  "yaml.customTags": [
    "!reference sequence"
  ],
  "yaml.validate": true,
  "yaml.completion": true,
  "problems.showCurrentInStatus": false,
  "typescript.tsserver.log": "off",
  "typescript.surveys.enabled": false,
  "typescript.suggest.completeJSDocs": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "never"
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "never"
    }
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "never"
    }
  },
  "[javascriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "never"
    }
  },
  "files.exclude": {
    "**/.git": true,
    "**/.next": true,
    "**/node_modules": false,
    "**/out": true,
    "**/build": true,
    "**/dist": true,
    "**/.turbo": true,
    "**/coverage": true,
    "**/*.tsbuildinfo": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/out/**": true,
    "**/build/**": true,
    "**/dist/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true,
    "**/out": true,
    "**/build": true,
    "**/dist": true
  }
}
