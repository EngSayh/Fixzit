{
  "files.exclude": {
    "**/_archive": true,
    "**/_artifacts": true,
    "**/coverage": true,
    "**/playwright-report": true,
    "**/*.zip": true,
    "**/*.tar.gz": true,
    "**/*.log": true,
    "**/*.bak": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.git/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/out/**": true,
    "**/build/**": true,
    "**/.turbo/**": true,
    "**/.cache/**": true
  },
  "search.exclude": {
    "**/_archive/**": true,
    "**/_artifacts/**": true,
    "**/node_modules": true,
    "**/.git": true,
    "**/.next": true,
    "**/dist": true,
    "**/out": true,
    "**/build": true,
    "**/.turbo": true,
    "**/.cache": true
  },
  "typescript.tsserver.maxTsServerMemory": 4096,
  "git.autofetch": false,
  "git.postCommitCommand": "none",
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {},
  "remote.SSH.serverAliveInterval": 30,
  "remote.SSH.serverAliveCountMax": 120
}
