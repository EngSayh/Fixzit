{
  "framework": "nextjs",
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm build",
  "outputDirectory": ".next",
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "regions": ["cdg1"],
  "crons": [{
    "path": "/api/cron",
    "schedule": "0 10 * * *"
  }]
}
