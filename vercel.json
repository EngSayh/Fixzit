{
  "framework": "nextjs",
  "installCommand": "corepack enable && corepack prepare pnpm@9.15.0 --activate && pnpm install --frozen-lockfile",
  "buildCommand": "pnpm build",
  "outputDirectory": ".next",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "regions": ["cdg1"],
  "crons": [{
    "path": "/api/cron",
    "schedule": "0 10 * * *"
  }]
}
