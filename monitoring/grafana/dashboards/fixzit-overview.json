{
  "annotations": {
    "list": [
      {
        "name": "Deployments",
        "datasource": "prometheus",
        "enable": true,
        "iconColor": "blue",
        "expr": "changes(deployment_timestamp[5m]) > 0"
      }
    ]
  },
  "description": "Fixzit Application Overview Dashboard",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Health Status",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "id": 1,
      "targets": [
        {
          "datasource": "prometheus",
          "expr": "up{job=\"fixzit\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "1": { "text": "UP", "color": "green" } } },
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 6, "y": 0 },
      "id": 2,
      "targets": [
        {
          "datasource": "prometheus",
          "expr": "sum(rate(http_requests_total{job=\"fixzit\"}[5m]))",
          "legendFormat": "Requests/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "reqps"
        }
      }
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 },
      "id": 3,
      "targets": [
        {
          "datasource": "prometheus",
          "expr": "sum(rate(http_requests_total{job=\"fixzit\", status=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"fixzit\"}[5m])) * 100",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 2 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "title": "Response Time (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "id": 4,
      "targets": [
        {
          "datasource": "prometheus",
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"fixzit\"}[5m])) by (le))",
          "legendFormat": "p95 Latency",
          "refId": "A"
        },
        {
          "datasource": "prometheus",
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"fixzit\"}[5m])) by (le))",
          "legendFormat": "p99 Latency",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1.5 },
              { "color": "red", "value": 2.5 }
            ]
          }
        }
      }
    },
    {
      "title": "Active Users",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 4 },
      "id": 5,
      "targets": [
        {
          "datasource": "prometheus",
          "expr": "sum(active_sessions{job=\"fixzit\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      }
    },
    {
      "title": "Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "id": 6,
      "targets": [
        {
          "datasource": "prometheus",
          "expr": "process_resident_memory_bytes{job=\"fixzit\"} / 1024 / 1024",
          "legendFormat": "RSS (MB)",
          "refId": "A"
        },
        {
          "datasource": "prometheus",
          "expr": "nodejs_heap_size_used_bytes{job=\"fixzit\"} / 1024 / 1024",
          "legendFormat": "Heap Used (MB)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "decmbytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 512 },
              { "color": "red", "value": 768 }
            ]
          }
        }
      }
    },
    {
      "title": "API Endpoints by Status",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 },
      "id": 7,
      "targets": [
        {
          "datasource": "prometheus",
          "expr": "sum by (status) (increase(http_requests_total{job=\"fixzit\"}[1h]))",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Top Endpoints by Request Count",
      "type": "table",
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 16 },
      "id": 8,
      "targets": [
        {
          "datasource": "prometheus",
          "expr": "topk(10, sum by (route) (increase(http_requests_total{job=\"fixzit\"}[1h])))",
          "format": "table",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["fixzit", "overview"],
  "templating": {
    "list": [
      {
        "name": "environment",
        "type": "query",
        "query": "label_values(up{job=\"fixzit\"}, environment)",
        "current": { "text": "production", "value": "production" }
      }
    ]
  },
  "time": {
    "from": "now-3h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Fixzit Overview",
  "uid": "fixzit-overview",
  "version": 1,
  "weekStart": ""
}
