    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/nextjs": "^10.25.0",
    "@types/redis": "^4.0.11",
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.28.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.2",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.6.0",
    "framer-motion": "^11.18.2",
    "helmet": "^7.2.0",
    "html2canvas": "^1.4.1",
    "ical.js": "^2.2.1",
    "import-in-the-middle": "1.15.0",
    "ioredis": "^5.8.2",
    "isomorphic-dompurify": "^2.32.0",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.25",
    "lru-cache": "^11.2.1",
    "lucide-react": "^0.294.0",
    "meilisearch": "^0.54.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.0",
    "nanoid": "^5.1.5",
    "nats": "^2.29.3",
    "next": "^15.5.4",
    "next-auth": "5.0.0-beta.30",
    "node-cron": "^4.2.1",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.4.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "require-in-the-middle": "7.5.2",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
