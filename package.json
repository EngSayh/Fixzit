    "globby": "^15.0.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^27.2.0",
    "k6": "0.0.0",
    "markdownlint-cli": "^0.45.0",
    "mongodb-memory-server": "^10.3.0",
    "ora": "^5.4.1",
